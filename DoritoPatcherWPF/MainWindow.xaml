                            </TextBlock>
                            <TextBlock FontSize="16" FontFamily="/Resources/#Aero Matics Light">Player Name:</TextBlock>
                            <TextBox Width="auto" Text="{Binding Player.Name}" FontSize="16" FontFamily="/Resources/#Aero Matics Light">
                                <TextBox.Background>
                                    <SolidColorBrush Color="White" Opacity="0.07"/>
                                </TextBox.Background>
                            </TextBox>

                            <TextBlock Style="{StaticResource Header}" Margin="5,20,5,5" FontFamily="/Resources/#Aero Matics" FontWeight="Normal">Armor Customization</TextBlock>
                            <TextBlock FontSize="16" FontFamily="/Resources/#Aero Matics Light">Helmet:</TextBlock>
                            <ComboBox x:Name="cmbHelmet" DropDownClosed="helmetClose" DropDownOpened="helmetOpen" ItemsSource="{Binding Player.Armor.HelmetChoices}" SelectedItem="{Binding Player.Armor.Helmet}" DisplayMemberPath="DisplayName" MaxDropDownHeight="393" FontSize="16" FontFamily="/Resources/#Aero Matics Light"/>
                            <TextBlock FontSize="16" FontFamily="/Resources/#Aero Matics Light">Chest:</TextBlock>
                            <ComboBox  x:Name="cmbChest" DropDownClosed="chestClose" DropDownOpened="chestOpen" ItemsSource="{Binding Player.Armor.ChestChoices}" SelectedItem="{Binding Player.Armor.Chest}" DisplayMemberPath="DisplayName" FontSize="16" FontFamily="/Resources/#Aero Matics Light"/>
                            <TextBlock FontSize="16" FontFamily="/Resources/#Aero Matics Light">Shoulders:</TextBlock>
                            <ComboBox x:Name="cmbShoulders" DropDownClosed="shouldersClose" DropDownOpened="shouldersOpen" ItemsSource="{Binding Player.Armor.ShoulderChoices}" SelectedItem="{Binding Player.Armor.Shoulders}" DisplayMemberPath="DisplayName" MaxDropDownHeight="325" FontSize="16" FontFamily="/Resources/#Aero Matics Light"/>
                            <TextBlock FontSize="16" FontFamily="/Resources/#Aero Matics Light">Arms:</TextBlock>
                            <ComboBox x:Name="cmbArms" DropDownClosed="armsClose" DropDownOpened="armsOpen" ItemsSource="{Binding Player.Armor.ArmChoices}" SelectedItem="{Binding Player.Armor.Arms}" DisplayMemberPath="DisplayName" MaxDropDownHeight="293" FontSize="16" FontFamily="/Resources/#Aero Matics Light"/>
                            <TextBlock FontSize="16" FontFamily="/Resources/#Aero Matics Light">Legs:</TextBlock>
                            <ComboBox x:Name="cmbLegs" ItemsSource="{Binding Player.Armor.LegChoices}" SelectedItem="{Binding Player.Armor.Legs}" DisplayMemberPath="DisplayName" MaxDropDownHeight="260" FontSize="16" FontFamily="/Resources/#Aero Matics Light"/>


                        </st:AutoGrid>
                        <st:AutoGrid Columns="auto,*" RowHeight="auto" Margin="0,20,20,5">
                            <TextBlock Style="{StaticResource Header}" FontFamily="/Resources/#Aero Matics" FontWeight="Normal">Armor Colors</TextBlock>
                            <TextBlock FontSize="16" FontFamily="/Resources/#Aero Matics Light">Primary:</TextBlock>
                            <xctk:ColorPicker x:Name="clrPrimary" SelectedColor="{Binding Player.Colors.Primary}" FontSize="16" FontFamily="/Resources/#Aero Matics Light">
                                <xctk:ColorPicker.Background>
                                    <SolidColorBrush Color="White" Opacity="0.07"/>
                                </xctk:ColorPicker.Background>
                            </xctk:ColorPicker>

                            <TextBlock FontSize="16" FontFamily="/Resources/#Aero Matics Light">Secondary:</TextBlock>
                            <xctk:ColorPicker SelectedColor="{Binding Player.Colors.Secondary}" FontSize="16" FontFamily="/Resources/#Aero Matics Light">
                                <xctk:ColorPicker.Background>
                                    <SolidColorBrush Color="White" Opacity="0.07"/>
                                </xctk:ColorPicker.Background>
                            </xctk:ColorPicker>
                            <TextBlock FontSize="16" FontFamily="/Resources/#Aero Matics Light">Visor:</TextBlock>
                            <xctk:ColorPicker SelectedColor="{Binding Player.Colors.Visor}" FontSize="16" FontFamily="/Resources/#Aero Matics Light">
                                <xctk:ColorPicker.Background>
                                    <SolidColorBrush Color="White" Opacity="0.07"/>
                                </xctk:ColorPicker.Background>
                            </xctk:ColorPicker>
                            <TextBlock FontSize="16" FontFamily="/Resources/#Aero Matics Light">Lights:</TextBlock>
                            <xctk:ColorPicker SelectedColor="{Binding Player.Colors.Lights}" FontSize="16" FontFamily="/Resources/#Aero Matics Light">
                                <xctk:ColorPicker.Background>
                                    <SolidColorBrush Color="White" Opacity="0.07"/>
                                </xctk:ColorPicker.Background>
                            </xctk:ColorPicker>
                            <TextBlock FontSize="16" FontFamily="/Resources/#Aero Matics Light">Holo:</TextBlock>
                            <xctk:ColorPicker SelectedColor="{Binding Player.Colors.Holo}" FontSize="16" FontFamily="/Resources/#Aero Matics Light">
                                <xctk:ColorPicker.Background>
                                    <SolidColorBrush Color="White" Opacity="0.07"/>
                                </xctk:ColorPicker.Background>
                            </xctk:ColorPicker>
                        </st:AutoGrid>
                    </st:AutoGrid>
                    <st:AutoGrid Columns="*" RowHeight="*" Margin="0,20,20,20" Height="30" VerticalAlignment="Bottom">
                        <Border BorderBrush="White" BorderThickness="1" Width="75" Height="30" HorizontalAlignment="Right">
                            <Border.Background>
                                <SolidColorBrush Color="White" Opacity="0.07"/>
                            </Border.Background>
                            <Button x:Name="btnApply" Click="btnApply_Click" Content="OK" Style="{StaticResource Hover}"/>
                        </Border>
                    </st:AutoGrid>
                    <Line Stretch="Fill" Y2="35" HorizontalAlignment="Right" VerticalAlignment="Top" Height="45" Width="45" Fill="{x:Null}" X2="35" Margin="0,-6,-5,0" Stroke="#FF47A6FF" StrokeThickness="2"/>
                </Grid>
            </Border>
            <Border x:Name="ChangelogGrid" Grid.Column="1" Grid.Row="1" BorderBrush="#FF47A6FF" BorderThickness="1" Margin="10,20,20,20" Visibility="Hidden">
                <Border.Clip>
                    <GeometryGroup>
                        <!--This PathGeometry will cut RectangleGeometry, cut-off right top-->
                        <PathGeometry>
                            <PathGeometry.Figures>
                                <PathFigure x:Name="BorderClipFigure9" StartPoint="0,0">
                                    <LineSegment Point="534,0" />
                                    <LineSegment Point="570,36" />
                                    <LineSegment Point="570,530" />
                                    <LineSegment Point="0,530" />
                                </PathFigure>
                            </PathGeometry.Figures>
                        </PathGeometry>
                    </GeometryGroup>
                </Border.Clip>
                <Grid Grid.ColumnSpan="2">
                    <Border Height="36" Margin="0" BorderBrush="#FF47A6FF" BorderThickness="0,0,0,1" VerticalAlignment="Top">
                        <Border.Background>
                            <SolidColorBrush Color="#FF20304A" Opacity="0.8"/>
                        </Border.Background>
                        <TextBlock Margin="20,0,0,0" Text="CHANGELOG" Foreground="White" TextWrapping="WrapWithOverflow"  FontFamily="/Resources/#Aero Matics" FontSize="20" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                    </Border>
                    <Grid Margin="0,36,0,0">
                        <Grid.Background>
                            <SolidColorBrush Color="White" Opacity="0.07"/>
                        </Grid.Background>
                        <TextBox x:Name="ChangelogContent" Margin="0" Background="Transparent" Foreground="White" BorderThickness="0" IsReadOnly="True" TextWrapping="WrapWithOverflow" VerticalScrollBarVisibility="Auto" AcceptsReturn="True" Text="Loading..." FontFamily="/Resources/#Aero Matics Light" FontSize="16" Padding="20,0" />
                        <st:AutoGrid Columns="*" RowHeight="*" Margin="0,20,20,20" Height="30" VerticalAlignment="Bottom">
                            <Border BorderBrush="White" BorderThickness="1" Width="75" Height="30" HorizontalAlignment="Right">
                                <Border.Background>
                                    <SolidColorBrush Color="White" Opacity="0.07"/>
                                </Border.Background>
                                <Button x:Name="btnOk" Click="btnOk_Click" Content="OK" Style="{StaticResource Hover}"/>
                            </Border>
                        </st:AutoGrid>
                    </Grid>
                    <Line Stretch="Fill" Y2="35" HorizontalAlignment="Right" VerticalAlignment="Top" Height="45" Width="45" Fill="{x:Null}" X2="35" Margin="0,-6,-5,0" Stroke="#FF47A6FF" StrokeThickness="2"/>
                </Grid>
            </Border>
            <Border x:Name="Settings" Grid.Column="1" Grid.Row="1" BorderBrush="#FF47A6FF" Margin="10,20,20,20" BorderThickness="1" Visibility="Hidden">
                <Border.Clip>
                    <GeometryGroup>
                        <!--This PathGeometry will cut RectangleGeometry, cut-off right top-->
                        <PathGeometry>
                            <PathGeometry.Figures>
                                <PathFigure x:Name="BorderClipFigure10" StartPoint="0,0">
                                    <LineSegment Point="534,0" />
                                    <LineSegment Point="570,36" />
                                    <LineSegment Point="570,530" />
                                    <LineSegment Point="0,530" />
                                </PathFigure>
                            </PathGeometry.Figures>
                        </PathGeometry>
                    </GeometryGroup>
                </Border.Clip>
                <Grid x:Name="GameSettings">
                    <Grid.Resources>
                        <Style TargetType="{x:Type TextBlock}">
                            <Setter Property="Foreground" Value="White"/>
                            <Setter Property="Margin" Value="5"/>
                            <Setter Property="VerticalAlignment" Value="Center"/>
                        </Style>
                        <Style x:Key="Header" TargetType="{x:Type TextBlock}" BasedOn="{StaticResource {x:Type TextBlock}}">
                            <Setter Property="FontWeight" Value="Bold"/>
                            <Setter Property="VerticalAlignment" Value="Top"/>
                            <Setter Property="Grid.ColumnSpan" Value="2"/>
                        </Style>
                        <Style TargetType="{x:Type TextBox}">
                            <Setter Property="Height" Value="23"/>
                            <Setter Property="Foreground" Value="White"/>
                            <Setter Property="BorderBrush" Value="White" />
                            <Setter Property="VerticalAlignment" Value="Center"/>
                            <Setter Property="Margin" Value="5"/>
                            <Setter Property="VerticalContentAlignment" Value="Center"/>
                            <Setter Property="HorizontalAlignment" Value="Stretch" />
                            <Setter Property="CaretBrush" Value="White"/>
                        </Style>
                        <Style TargetType="{x:Type ComboBox}" BasedOn="{StaticResource {x:Type ComboBox}}">
                            <Setter Property="Height" Value="23"/>
                            <Setter Property="VerticalAlignment" Value="Center"/>
                            <Setter Property="Margin" Value="5"/>
                            <Setter Property="VerticalContentAlignment" Value="Center"/>
                            <Setter Property="HorizontalAlignment" Value="Stretch" />
                            <Setter Property="Foreground" Value="White"/>
                        </Style>
                        <Style TargetType="{x:Type Slider}">
                            <Setter Property="Height" Value="23"/>
                            <Setter Property="Foreground" Value="White"/>
                            <Setter Property="Background" Value="Transparent"/>
                            <Setter Property="VerticalAlignment" Value="Center"/>
                            <Setter Property="Margin" Value="5"/>
                            <Setter Property="VerticalContentAlignment" Value="Center"/>
                            <Setter Property="TickPlacement" Value="BottomRight"/>
                            <Setter Property="HorizontalAlignment" Value="Stretch"/>
                        </Style>
                        <Style TargetType="{x:Type xctk:ColorPicker}">
                            <Setter Property="Height" Value="23"/>
                            <Setter Property="Foreground" Value="White"/>
                            <Setter Property="Background" Value="Transparent"/>
                            <Setter Property="VerticalAlignment" Value="Center"/>
                            <Setter Property="Margin" Value="5"/>
                            <Setter Property="ColorMode" Value="ColorCanvas"/>
                            <Setter Property="UsingAlphaChannel" Value="False"/>
                            <Setter Property="ShowDropDownButton" Value="False"/>
                            <Setter Property="DisplayColorAndName" Value="True"/>
                            <Setter Property="Template" Value="{StaticResource ColorPickerControlTemplate}"/>
                            <Setter Property="HorizontalAlignment" Value="Stretch"/>
                            <Setter Property="BorderBrush">
                                <Setter.Value>
                                    <SolidColorBrush Color="White" Opacity="0.7"/>
                                </Setter.Value>
                            </Setter>
                        </Style>
                        <Style TargetType="{x:Type CheckBox}">
                            <Setter Property="Height" Value="23"/>
                            <Setter Property="Foreground" Value="White"/>
                            <Setter Property="Background" Value="Transparent"/>
                            <Setter Property="BorderBrush" Value="White"/>
                            <Setter Property="VerticalAlignment" Value="Center"/>
                            <Setter Property="Margin" Value="5"/>
                            <Setter Property="VerticalContentAlignment" Value="Center"/>
                            <Setter Property="HorizontalAlignment" Value="Stretch" />
                        </Style>
                    </Grid.Resources>
                    <Border VerticalAlignment="Top" Height="36" Margin="0" BorderBrush="#FF47A6FF" BorderThickness="0,0,0,1">
                        <Border.Background>
                            <SolidColorBrush Color="#FF20304A" Opacity="0.8"/>
                        </Border.Background>
                        <TextBlock Margin="20,0,0,0" Text="SETTINGS" Foreground="White" TextWrapping="WrapWithOverflow"  FontFamily="/Resources/#Aero Matics" FontSize="20" HorizontalAlignment="Left"/>
                    </Border>
                    <st:AutoGrid Columns="*,*" Margin="0,36,0,0">
                        <st:AutoGrid.Background>
                            <SolidColorBrush Color="White" Opacity="0.07"/>
                        </st:AutoGrid.Background>
                        <st:AutoGrid Columns="auto,*" RowHeight="auto" Margin="5,20,25,5">
                            <TextBlock Style="{StaticResource Header}" FontFamily="/Resources/#Aero Matics" Margin="5,0,5,5" FontWeight="Normal">Graphics Settings</TextBlock>
                            <TextBlock FontSize="16" FontFamily="/Resources/#Aero Matics Light">Field of View:</TextBlock>
                            <DockPanel>
                                <TextBox DockPanel.Dock="Right" Text="{Binding Video.FieldOfView}" Width="60" TextAlignment="Center" FontFamily="/Resources/#Aero Matics Light">
                                    <TextBox.Background>
                                        <SolidColorBrush Color="White" Opacity="0.07"/>
                                    </TextBox.Background>
                                </TextBox>
                                <Slider Name="sldFov" DockPanel.Dock="Left" Minimum="30" Maximum="150" TickFrequency="5" IsSnapToTickEnabled="True" Value="{Binding Video.FieldOfView}" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="White"/>
                            </DockPanel>

                            <CheckBox Name="chkCenter" Grid.ColumnSpan="2" IsChecked="{Binding Video.CrosshairCentered}" Template="{DynamicResource CheckBoxControlTemplate1}" FontSize="16" FontFamily="/Resources/#Aero Matics Light">
                                Centered Crosshair
                            </CheckBox>

                            <TextBlock Style="{StaticResource Header}" Margin="5,25,5,5" FontFamily="/Resources/#Aero Matics" FontWeight="Normal">Input Settings</TextBlock>
                            <CheckBox Name="chkRaw" Grid.ColumnSpan="2" IsChecked="{Binding Input.RawMouse}" Template="{DynamicResource CheckBoxControlTemplate1}" FontSize="16" FontFamily="/Resources/#Aero Matics Light">
                                Use Raw Mouse Input
                            </CheckBox>
                        </st:AutoGrid>
                        <st:AutoGrid Columns="auto,*" RowHeight="auto" Margin="5,20,5,5">
                            <TextBlock Style="{StaticResource Header}" FontFamily="/Resources/#Aero Matics" Margin="5,0,5,5" FontWeight="Normal">Host Settings</TextBlock>
                            <TextBlock FontSize="16" FontFamily="/Resources/#Aero Matics Light">Start Timer:</TextBlock>
                            <DockPanel>
                                <TextBox DockPanel.Dock="Right" Text="{Binding Host.Countdown}" Width="60" TextAlignment="Center" FontFamily="/Resources/#Aero Matics Light">
                                    <TextBox.Background>
                                        <SolidColorBrush Color="White" Opacity="0.07"/>
                                    </TextBox.Background>
                                </TextBox>
                                <Slider Name="sldTimer" DockPanel.Dock="Left" Minimum="0" Maximum="20" TickFrequency="1" IsSnapToTickEnabled="True" Value="{Binding Host.Countdown}" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="White" FontFamily="/Resources/#Aero Matics Light"/>
                            </DockPanel>
                            <!--
                            <TextBlock FontSize="16" FontFamily="/Resources/#Aero Matics Light">Maximum Players:</TextBlock>
                            <DockPanel>
                                <TextBox DockPanel.Dock="Right" Text="{Binding Host.MaxPlayer}" Width="60" TextAlignment="Center" FontFamily="/Resources/#Aero Matics Light">
                                    <TextBox.Background>
                                        <SolidColorBrush Color="White" Opacity="0.07"/>
                                    </TextBox.Background>
                                </TextBox>
                                <Slider Name="sldMax" DockPanel.Dock="Left" Minimum="1" Maximum="16" TickFrequency="1" IsSnapToTickEnabled="True" Value="{Binding Host.MaxPlayer}" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="White" FontFamily="/Resources/#Aero Matics Light"/>
                            </DockPanel>
                            -->
                        </st:AutoGrid>
                    </st:AutoGrid>
                    <st:AutoGrid Columns="*" RowHeight="*" Margin="0,20,20,20" Height="30" VerticalAlignment="Bottom">
                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                            <Border Grid.Column="1" Grid.Row="1" BorderBrush="White" BorderThickness="1" Width="75" Height="30" HorizontalAlignment="Right">
                                <Border.Background>
                                    <SolidColorBrush Color="White" Opacity="0.07"/>
                                </Border.Background>
                                <Button x:Name="btnReset" Content="RESET" Click="btnReset_Click" Style="{StaticResource Hover}"/>
                            </Border>
                            <Border Grid.Column="1" Grid.Row="1" BorderBrush="White" BorderThickness="1" Width="75" Height="30"  Margin="20,0,0,0" HorizontalAlignment="Right">
                                <Border.Background>
                                    <SolidColorBrush Color="White" Opacity="0.07"/>
                                </Border.Background>
                                <Button x:Name="btnApply2" Content="OK" Click="btnApply2_Click" Style="{StaticResource Hover}"/>
                            </Border>
                        </StackPanel>
                    </st:AutoGrid>
                    <Line Stretch="Fill" Y2="35" HorizontalAlignment="Right" VerticalAlignment="Top" Height="45" Width="45" Fill="{x:Null}" X2="35" Margin="0,-6,-5,0" Stroke="#FF47A6FF" StrokeThickness="2"/>
                </Grid>
            </Border>
        </Grid>
        <!--<Canvas HorizontalAlignment="Left" Height="230" Margin="10,10,0,0" VerticalAlignment="Top" Width="484">
            <Canvas.Background>
                <ImageBrush ImageSource="Resources/logo.png" Stretch="Uniform"/>
            </Canvas.Background>
        </Canvas>-->
    </Grid>
</Window>
