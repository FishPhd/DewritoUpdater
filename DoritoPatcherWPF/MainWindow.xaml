<Window x:Class="DoritoPatcherWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:osm="http://schemas.awesomium.com/winfx"
        xmlns:core="clr-namespace:Awesomium.Core;assembly=Awesomium.Core"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
        xmlns:st="http://schemas.spicytaco.io/"
        xmlns:utils="clr-namespace:DoritoPatcherWPF.Utils"
        xmlns:local="clr-namespace:DoritoPatcherWPF"
        mc:Ignorable="d" 
        Title="ElDewrito Launcher" Height="600" Width="1000" ResizeMode="CanMinimize" Icon="/Resources/launcher.ico"
        WindowStartupLocation="CenterScreen" WindowStyle="None" AllowsTransparency="False"
        utils:WindowMovement.IsDraggable="True"
        Loaded="Window_Loaded"
        UseLayoutRounding="True" 
        FontFamily="/Resources/#Aero Matics Light" TextOptions.TextFormattingMode="Display" FontSize="18">
    <Window.Resources>
        <osm:WebSessionProvider x:Key="session">
            <core:WebPreferences 
                SmoothScrolling="True"
                EnableGPUAcceleration="True"
                WebGL="True"
                CustomCSS = "body 
{ 
    font-family: Aero Matics Light !important;
    letter-spacing: 1px !important;
    background: transparent !important; 
    padding: 0 !important; 
} 

b, strong
{
    font-family: Aero Matics !important;
    font-weight: 400 !important;
}

.navbar 
{ 
    display: none !important;
} 

.main 
{ 
    padding: 20px 10px !important; 
} 

.panel
{
    opacity: 1 !important;
    background: rgba(33, 77, 117, 0.85) !important;
    border: 1px solid #47a6ff !important;
    border-radius: 0 !important;
}

.panel-heading, .panel-default>.panel-heading
{
    opacity: 1 !important;
    font-family: Aero Matics !important;
    color: white !important;
    text-transform: uppercase !important;
    background: rgba(255, 255, 255, 0.07) !important;
    border-radius: 0 !important;
}

.panel .panel
{
    background: rgba(33, 77, 117, 0) !important;
    border: none !important;
    border-radius: 0 !important;
    box-shadow: none !important;
}

.panel .panel-heading, .panel .panel-default>.panel-heading
{
    opacity: 1 !important;
    font-family: Aero Matics !important;
    color: white !important;
    text-transform: uppercase !important;
    background: rgba(255, 255, 255, 0.07) !important;
    border-radius: 0 !important;
}

.panel-body
{
    color: white !important;
}

.map-container 
{ 
    height : 350px !important; 
} 

.reactiveAds 
{ 
    display : none !important; 
} 

::-webkit-scrollbar 
{ 
    width: 18px !important; 
    background-clip: padding-box !important; 
} 

::-webkit-scrollbar-track 
{ 
    cursor: pointer !important; 
} 

::-webkit-scrollbar-thumb 
{ 
    background: rgba(255, 255, 255, 0.3) !important; 
    border-right: 10px solid rgba(0, 0, 0, 0) !important; 
    border-left: 3px solid rgba(0, 0, 0, 0) !important;
    background-clip: padding-box !important;
} 

::-webkit-scrollbar-thumb:hover 
{ 
    background: rgba(255, 255, 255, 0.7) !important; 
    border-right: 10px solid rgba(0, 0, 0, 0) !important; 
    border-left: 0px solid rgba(0, 0, 0, 0) !important;
    background-clip: padding-box !important; 
} 

::-webkit-scrollbar-button:vertical:decrement, ::-webkit-scrollbar-button:horizontal:decrement 
{ 
    height: 10px !important; 
    width: 20px !important; 
} 

::-webkit-scrollbar-button:vertical:increment, ::-webkit-scrollbar-button:horizontal:increment 
{ 
    height: 20px !important; 
    width: 20px !important; 
}

.table {
  width: 530px !important;
  max-width: none !important;
  margin: 0 -15px 20px !important;
}

#playerTable
{
  margin: 0 0 40px !important;
}

.table-striped>tbody>tr:nth-of-type(odd) {
    background: rgba(255, 255, 255, 0.07) !important;
}

.table-striped>tbody>tr:nth-of-type(even) {
    background: rgba(0, 0, 0, 0.07) !important;
}

td
{
    font-size: 16px !important;
}

.table-responsive {
    background: none !important;
    border: none !important;
    width: 530px !important;
    margin: 0 -15px !important;
}

.well {
    background: none !important;
    border: none !important;
    box-shadow: none !important;
    padding: 0 !important;
}

h3.sub-header.text-center {
    font-family: aero matics !important;
    text-transform: uppercase !important;
    color: white !important;
    border: none !important;
    padding: 10px 0 0 !important;
}

.chartPadding {
    padding: 10px 20px !important;
    margin: -15px -15px -35px !important;
    width: 529px !important;
    background: rgba(255, 255, 255, 0.77) !important;
}"
                />
        </osm:WebSessionProvider>
        <Storyboard x:Key="fade" RepeatBehavior="Forever">
            <DoubleAnimation Storyboard.TargetName="btnAction" 
        		Storyboard.TargetProperty="Opacity" From="0.20" To="1" 
        		Duration="0:0:1" BeginTime="0:0:0" AutoReverse="True" />
        </Storyboard>
        <Storyboard x:Key="fadePanels">
            <DoubleAnimation Storyboard.TargetName="contentPanels" 
        		Storyboard.TargetProperty="Opacity" From="1" To="0" 
        		Duration="0:0:0.150" BeginTime="0:0:0"  />
        </Storyboard>
        <Storyboard x:Key="showPanels">
            <DoubleAnimation Storyboard.TargetName="contentPanels" 
        		Storyboard.TargetProperty="Opacity" From="0" To="1" 
        		Duration="0:0:0.150" BeginTime="0:0:0"  />
        </Storyboard>
        <!--
        <Storyboard x:Key="fadeServer" RepeatBehavior="Forever">
            <DoubleAnimation Storyboard.TargetName="lblVerify" 
        		Storyboard.TargetProperty="Opacity" From="0.20" To="1" 
        		Duration="0:0:1" BeginTime="0:0:0" AutoReverse="True" />
        </Storyboard>
        <Storyboard x:Key="fadeStat" RepeatBehavior="Forever">
            <DoubleAnimation Storyboard.TargetName="lblVerify2" 
        		Storyboard.TargetProperty="Opacity" From="0.20" To="1" 
        		Duration="0:0:1" BeginTime="0:0:0" AutoReverse="True" />
        </Storyboard>
        -->
    </Window.Resources>
    <Window.Background>
        <ImageBrush ImageSource="Resources/BG_Nightfall.png"/>
    </Window.Background>
    <Grid>
        <Grid.Resources>
            <Style TargetType="{x:Type Button}">
                <Setter Property="ToolTipService.InitialShowDelay" Value="2000"/>
            </Style>
        </Grid.Resources>
        <Grid.RowDefinitions>
            <RowDefinition Height="30"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="2*"/>
            <ColumnDefinition Width="3*"/>
        </Grid.ColumnDefinitions>
        <TextBlock TextWrapping="Wrap" Margin="20,3,398,0" Foreground="White" Grid.ColumnSpan="2" FontSize="20" TextOptions.TextFormattingMode="Display">
            <Run Foreground="White" Text="ELDEWRITO" FontFamily="/Resources/#Aero Matics" BaselineAlignment="Center"/>
            <Run Foreground="White" Text="- HALO ONLINE" BaselineAlignment="Center"/>
        </TextBlock>
        <Grid Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" DockPanel.Dock="Left" Background="Transparent" utils:WindowMovement.DragsWindow="True" Height="30" VerticalAlignment="Top"/>
        <StackPanel Grid.Row="0" Grid.Column="1" DockPanel.Dock="Right" Orientation="Horizontal" HorizontalAlignment="Right" Width="114">
            <Button BorderBrush="#FF707070" BorderThickness="1" FontSize="20" Width="57" Click="MinButton_Click" Style="{StaticResource HoverMinimize}" Cursor="Hand" ToolTip="Minimize the launcher">
                <Image Source="Resources/BTN_Minimize.png" Width="14" HorizontalAlignment="Center" VerticalAlignment="Center" />
            </Button>
            <Button BorderBrush="#FF707070" BorderThickness="1" FontSize="15" Width="57" Click="CloseButton_Click" Style="{StaticResource HoverClose}" Cursor="Hand" ToolTip="Close the launcher">
                <Image Source="Resources/BTN_Close.png" Width="14" Height="14" HorizontalAlignment="Center" VerticalAlignment="Center" />
            </Button>
        </StackPanel>
        <Grid Grid.Row="1" Grid.Column="0" Margin="20,20,10,20">
            <Canvas HorizontalAlignment="Center" Height="374" VerticalAlignment="Center" Width="370" SnapsToDevicePixels="True" RenderOptions.BitmapScalingMode="NearestNeighbor" Margin="0,12,0,144">
                <Canvas.Background>
                    <ImageBrush ImageSource="Resources/logo.png" Stretch="UniformToFill"/>
                </Canvas.Background>
            </Canvas>
            <Grid Height="108" VerticalAlignment="Bottom">
                <Border BorderThickness="2" Width="215" HorizontalAlignment="Left" Margin="0" Height="54" VerticalAlignment="Top">
                    <Border.Style>
                        <Style TargetType="Border">
                            <Setter Property="BorderBrush" Value="#FF47A6FF">
                            </Setter>
                            <Setter Property="Background">
                                <Setter.Value>
                                    <SolidColorBrush Color="#FF214D75" Opacity="1"/>
                                </Setter.Value>
                            </Setter>
                            <Style.Triggers>
                                <Trigger Property="IsMouseOver" Value="True">
                                    <Setter Property="BorderBrush" Value="#FFB3DAFF" />
                                    <Setter Property="Background" Value="#FF567EA2" />
                                </Trigger>
                            </Style.Triggers>
                        </Style>
                    </Border.Style>
                    <Border.Clip>
                        <GeometryGroup>
                            <!--This PathGeometry will cut RectangleGeometry, cut-off right top-->
                            <PathGeometry>
                                <PathGeometry.Figures>
                                    <PathFigure x:Name="BorderClipFigure" StartPoint="0,0">
                                        <LineSegment Point="195,0" />
                                        <LineSegment Point="215,20" />
                                        <LineSegment Point="215,55" />
                                        <LineSegment Point="0,55" />
                                    </PathFigure>
                                </PathGeometry.Figures>
                            </PathGeometry>
                        </GeometryGroup>
                    </Border.Clip>
                    <Grid Name="gridAction">
                        <Grid.Resources>
                            <Style TargetType="{x:Type Line}">
                                <Setter Property="Stroke" Value="#FF47A6FF" />
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding RelativeSource={RelativeSource AncestorType=Grid},
                                               Path=IsMouseOver}" Value="True">
                                        <Setter Property="Stroke" Value="#FFB3DAFF" />
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </Grid.Resources>
                        <Button x:Name="btnAction" Content="VERIFYING" HorizontalAlignment="Center" IsEnabled="False" Click="btnAction_Click"  Template="{DynamicResource BackgroundButton}" BorderBrush="White" FontSize="30" Width="215" FontFamily="/Resources/#Aero Matics" Cursor="Hand">
                            <Button.Background>
                                <SolidColorBrush Color="#00FF0000"/>
                            </Button.Background>
                            <Button.Resources>
                                <ControlTemplate x:Key="BackgroundButton" TargetType="Button">
                                    <Border Name="border" Background="{TemplateBinding Background}">
                                        <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center" />
                                    </Border>
                                </ControlTemplate>
                            </Button.Resources>
                            <Button.Style>
                                <Style TargetType="{x:Type Button}">
                                    <Setter Property="Foreground" Value="White"/>
                                    <Setter Property="Template">
                                        <Setter.Value>
                                            <ControlTemplate TargetType="{x:Type Button}">
                                                <Border Background="{TemplateBinding Background}">
                                                    <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                                </Border>
                                            </ControlTemplate>
                                        </Setter.Value>
                                    </Setter>
                                    <Style.Triggers>
                                        <DataTrigger Binding="{Binding RelativeSource={RelativeSource AncestorType=Border,AncestorLevel=1}, Path=IsMouseOver}" Value="True" >
                                            <Setter Property="Background" Value="White" />
                                        </DataTrigger>
                                    </Style.Triggers>
                                </Style>
                            </Button.Style>
                        </Button>
                        <Line Stretch="Fill" Y2="35" HorizontalAlignment="Right" VerticalAlignment="Top" Height="25" Width="25" Fill="{x:Null}" X2="35" Margin="0,-4,-4,0" StrokeThickness="3"/>
                    </Grid>
                </Border>
                <StackPanel HorizontalAlignment="Left" Height="54" Margin="222,0,-7,0" VerticalAlignment="Top" Width="155" Orientation="Horizontal">
                    <Button Name="btnSettings" Click="btnSettings_Click" BorderBrush="{x:Null}" VerticalAlignment="Bottom" Background="#00000000" Template="{DynamicResource BackgroundButton}" Margin="0" Height="54" Width="51.6" UseLayoutRounding="False" IsHitTestVisible="True" Cursor="Hand" ToolTip="Change game or launcher settings">
                        <Button.Resources>
                            <ControlTemplate x:Key="BackgroundButton" TargetType="Button">
                                <Border Name="border" Background="{TemplateBinding Background}">
                                    <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center" />
                                </Border>
                                <ControlTemplate.Triggers>
                                    <Trigger Property="IsMouseOver" Value="True">
                                    </Trigger>
                                </ControlTemplate.Triggers>
                            </ControlTemplate>
                            <Style TargetType="{x:Type Image}">
                                <Setter Property="Source" Value="Resources/BTN_Settings.png" />
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding RelativeSource={RelativeSource AncestorType=Button}, Path=IsMouseOver}" Value="True">
                                        <Setter Property="Source" Value="Resources/BTN_Settings_Blue.png"/>
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </Button.Resources>
                        <Image Height="26" HorizontalAlignment="Center" Width="26" />
                    </Button>
                    <Button Name="btnReddit" Click="btnReddit_Click" BorderBrush="{x:Null}" VerticalAlignment="Bottom" Background="#00000000" Template="{DynamicResource BackgroundButton}" Margin="0" Height="54" UseLayoutRounding="False" Width="51.6" Cursor="Hand" ToolTip="Open the reddit page in your browser">
                        <Button.Resources>
                            <ControlTemplate x:Key="BackgroundButton" TargetType="Button">
                                <Border Name="border" Background="{TemplateBinding Background}">
                                    <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center" />
                                </Border>
                                <ControlTemplate.Triggers>
                                    <Trigger Property="IsMouseOver" Value="True">
                                    </Trigger>
                                </ControlTemplate.Triggers>
                            </ControlTemplate>
                            <Style TargetType="{x:Type Image}">
                                <Setter Property="Source" Value="Resources/BTN_Reddit.png" />
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding RelativeSource={RelativeSource AncestorType=Button}, Path=IsMouseOver}" Value="True">
                                        <Setter Property="Source" Value="Resources/BTN_Reddit_Blue.png"/>
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </Button.Resources>
                        <Image HorizontalAlignment="Center" VerticalAlignment="Center" Height="26" />
                    </Button>
                    <Button Name="btnInfo" Click="btnInfo_Click" BorderBrush="{x:Null}" VerticalAlignment="Bottom" Background="#00000000" Template="{DynamicResource BackgroundButton}" Margin="0" Height="54" UseLayoutRounding="False" Width="51.6" Cursor="Hand" ToolTip="Information about stuff." Visibility="Collapsed">
                        <Button.Resources>
                            <ControlTemplate x:Key="BackgroundButton" TargetType="Button">
                                <Border Name="border" Background="{TemplateBinding Background}">
                                    <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center" />
                                </Border>
                                <ControlTemplate.Triggers>
                                    <Trigger Property="IsMouseOver" Value="True">
                                    </Trigger>
                                </ControlTemplate.Triggers>
                            </ControlTemplate>
                            <Style TargetType="{x:Type Image}">
                                <Setter Property="Source" Value="Resources/BTN_Help.png" />
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding RelativeSource={RelativeSource AncestorType=Button}, Path=IsMouseOver}" Value="True">
                                        <Setter Property="Source" Value="Resources/BTN_Help_Blue.png"/>
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </Button.Resources>
                        <Image HorizontalAlignment="Center" VerticalAlignment="Center" Height="26" Width="26" />
                    </Button>
                    <Button Name="btnDebug" Click="btnDebug_Click" BorderBrush="{x:Null}" VerticalAlignment="Bottom" Background="#00000000" Template="{DynamicResource BackgroundButton}" Margin="0" Height="54" UseLayoutRounding="False" Width="51.6" Cursor="Hand" ToolTip="View the debug logs for debugging purposes.">
                        <Button.Resources>
                            <ControlTemplate x:Key="BackgroundButton" TargetType="Button">
                                <Border Name="border" Background="{TemplateBinding Background}">
                                    <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center" />
                                </Border>
                                <ControlTemplate.Triggers>
                                    <Trigger Property="IsMouseOver" Value="True">
                                    </Trigger>
                                </ControlTemplate.Triggers>
                            </ControlTemplate>
                            <Style TargetType="{x:Type Image}">
                                <Setter Property="Source" Value="Resources/BTN_Debug.png" />
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding RelativeSource={RelativeSource AncestorType=Button}, Path=IsMouseOver}" Value="True">
                                        <Setter Property="Source" Value="Resources/BTN_Debug_Blue.png"/>
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </Button.Resources>
                        <Image HorizontalAlignment="Center" VerticalAlignment="Center" Height="26" Width="26" Opacity="0.9" />
                    </Button>
                </StackPanel>
                <Button Name="btnChangelog" Click="btnChangelog_Click" BorderBrush="{x:Null}" VerticalAlignment="Top" Background="#00000000" Template="{DynamicResource BackgroundButton}" Width="370" HorizontalAlignment="Left" Margin="0,60,0,0" Cursor="Hand" ToolTip="View the changelist">
                    <Button.Resources>
                        <ControlTemplate x:Key="BackgroundButton" TargetType="Button">
                            <Border Name="border" BorderThickness="0" Background="{TemplateBinding Background}">
                                <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center" />
                            </Border>
                            <ControlTemplate.Triggers>
                                <Trigger Property="IsMouseOver" Value="True">
                                </Trigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Button.Resources>
                    <StackPanel Height="35" Orientation="Horizontal">
                        <StackPanel.Resources>
                            <Style TargetType="{x:Type Image}">
                                <Setter Property="Source" Value="Resources/BTN_Changelog.png" />
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding RelativeSource={RelativeSource AncestorType=Button}, Path=IsMouseOver}" Value="True">
                                        <Setter Property="Source" Value="Resources/BTN_Changelog_Blue.png"/>
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>

                            <Style TargetType="{x:Type TextBlock}">
                                <Setter Property="Foreground" Value="#ffffffff" />
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding RelativeSource={RelativeSource AncestorType=Button},
                                                   Path=IsMouseOver}" Value="True">
                                        <Setter Property="Foreground" Value="#FF47A6FF" />
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </StackPanel.Resources>
                        <Image Height="18" HorizontalAlignment="Left" Margin="0" Width="20" />
                        <TextBlock TextWrapping="Wrap" Margin="10,-8,580,-7" Grid.ColumnSpan="2" FontSize="21" TextOptions.TextFormattingMode="Display" VerticalAlignment="Center" Width="350">
                            <Run Text="CURRENT VERSION " BaselineAlignment="Center"/>
                            <Run x:Name="lblVersion" Text="0.4.5-cf04b81b " FontFamily="/Resources/#Aero Matics" BaselineAlignment="Center"/>
                        </TextBlock>
                    </StackPanel>
                </Button>
                <!-- Do not remove this, Fish, I'm looking at you -->
                <TextBlock TextWrapping="Wrap" Foreground="White" FontSize="9" TextOptions.TextFormattingMode="Display" VerticalAlignment="Bottom" Height="10" Opacity="0.5" Margin="0,0,-78,0" FontFamily="/DoritoPatcherWPF;component/Resources/#Aero Matics Light" >
                    <Run Text="THIS PROGRAM IS IN NO WAY AFFILIATED WITH INNOVA CO., 343 INDUSTRIES, MICROSOFT OR BUNGIE STUDIOS"/>
                </TextBlock>
            </Grid>
            <Label x:Name="minecraft" Content="NOW WITH MORE ANIMATIONS" HorizontalAlignment="Left" Margin="56,103,-23,0" VerticalAlignment="Top" Foreground="#FFFFE800" RenderTransformOrigin="0.5,0.5" FontFamily="Minecraftia" Height="54" Width="337" HorizontalContentAlignment="Center" Visibility="Hidden">
                <Label.Style>
                    <Style TargetType="{x:Type Label}">
                        <Setter Property="FontSize" Value="18" />
                        <Style.Triggers>
                            <DataTrigger Binding="{Binding RelativeSource={RelativeSource AncestorType=Grid},
                                                   Path=IsMouseOver}" Value="True">
                                <DataTrigger.EnterActions>
                                    <BeginStoryboard>
                                        <Storyboard RepeatBehavior="Forever">
                                            <DoubleAnimation 
        		                                Storyboard.TargetProperty="FontSize" From="18.1" To="20.1" 
        		                                Duration="0:0:0.500" BeginTime="0:0:0" AutoReverse="True"/>
                                        </Storyboard>
                                    </BeginStoryboard>
                                </DataTrigger.EnterActions>
                            </DataTrigger>
                        </Style.Triggers>
                    </Style>
                </Label.Style>
                <Label.Effect>
                    <DropShadowEffect BlurRadius="0" ShadowDepth="3"/>
                </Label.Effect>
                <Label.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform Angle="27.26"/>
                        <TranslateTransform/>
                    </TransformGroup>
                </Label.RenderTransform>
            </Label>
        </Grid>


        <!-- Right column starts here -->
        <Grid x:Name="contentPanels" Grid.Column="1" Grid.Row="1">
            <Grid x:Name="mainButtons" Grid.Column="1" Grid.Row="1">
                <st:AutoGrid Grid.Row="0" Grid.Column="0" Rows="*,*,*,*" Columns="*" Margin="10,20,20,20">
                    <Grid Grid.Row="1" Margin="0,0,0,5" VerticalAlignment="Top">
                        <Grid.Style>
                            <Style TargetType="{x:Type Grid}">
                                <Style.Resources>
                                    <Style TargetType="{x:Type Border}">
                                        <Setter Property="BorderBrush" Value="#FF47A6FF" />
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding RelativeSource={RelativeSource AncestorType=Grid},
                                                   Path=IsMouseOver}" Value="True">
                                                <Setter Property="BorderBrush" Value="#FFB3DAFF" />
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </Style.Resources>
                            </Style>
                        </Grid.Style>
                        <Border BorderThickness="1">
                            <Border.Clip>
                                <GeometryGroup>
                                    <!--This PathGeometry will cut RectangleGeometry, cut-off right top-->
                                    <PathGeometry>
                                        <PathGeometry.Figures>
                                            <PathFigure x:Name="BorderClipFigure2" StartPoint="0,0">
                                                <LineSegment Point="534,0" />
                                                <LineSegment Point="570,36" />
                                                <LineSegment Point="570,130" />
                                                <LineSegment Point="0,130" />
                                            </PathFigure>
                                        </PathGeometry.Figures>
                                    </PathGeometry>
                                </GeometryGroup>
                            </Border.Clip>
                            <Button x:Name="btnServer" Click="btnServer_Click" Template="{DynamicResource BackgroundButton}" BorderThickness="0" BorderBrush="{x:Null}" Margin="0" Background="{x:Null}" Cursor="Hand" ToolTip="Launch the server browser to look for games">
                                <Button.Resources>
                                    <ControlTemplate x:Key="BackgroundButton" TargetType="Button">
                                        <Border Name="border" BorderThickness="0" Background="{TemplateBinding Background}">
                                            <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center" />
                                        </Border>
                                        <ControlTemplate.Triggers>
                                            <Trigger Property="IsMouseOver" Value="True">
                                            </Trigger>
                                        </ControlTemplate.Triggers>
                                    </ControlTemplate>
                                    <Style TargetType="{x:Type Line}">
                                        <Setter Property="Stroke" Value="#FF47A6FF" />
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding RelativeSource={RelativeSource AncestorType=Grid},
                                                   Path=IsMouseOver}" Value="True">
                                                <Setter Property="Stroke" Value="#FFB3DAFF" />
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                    <Style TargetType="{x:Type Label}">
                                        <Setter Property="Margin" Value="20,0,0,15" />
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding RelativeSource={RelativeSource AncestorType=Grid},
                                                   Path=IsMouseOver}" Value="True">
                                                <DataTrigger.EnterActions>
                                                    <BeginStoryboard>
                                                        <Storyboard>
                                                            <ThicknessAnimation Duration="0:0:0.150" To="30,0,0,15" Storyboard.TargetProperty="Margin" />
                                                        </Storyboard>
                                                    </BeginStoryboard>
                                                </DataTrigger.EnterActions>
                                                <DataTrigger.ExitActions>
                                                    <BeginStoryboard>
                                                        <Storyboard>
                                                            <ThicknessAnimation Duration="0:0:0.200" To="20,0,0,15" Storyboard.TargetProperty="Margin" />
                                                        </Storyboard>
                                                    </BeginStoryboard>
                                                </DataTrigger.ExitActions>
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </Button.Resources>
                                <Grid Width="570" Height="125">
                                    <Grid.Background>
                                        <ImageBrush ImageSource="Resources/Browser.png" Stretch="UniformToFill"/>
                                    </Grid.Background>
                                    <Label x:Name="lblServers" Content="SERVER BROWSER" FontSize="25" FontFamily="Aero Matics" Background="{x:Null}" HorizontalAlignment="Left" VerticalAlignment="Bottom" VerticalContentAlignment="Bottom" Padding="0,5,5,0" Foreground="White">
                                        <Label.Effect>
                                            <DropShadowEffect BlurRadius="10" ShadowDepth="0" Opacity="0.7"/>
